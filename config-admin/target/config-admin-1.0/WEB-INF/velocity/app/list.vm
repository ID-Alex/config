
<form id="form" class="form-inline">

    <button type="button" class="btn btn-info" onclick="add()">添加应用</button>
</form>

<table class="table">
    <thead>
    <tr>
        <th>ID</th>
        <th>应用名称</th>
        <th>创建人</th>
        <th>创建时间</th>
    </tr>
    </thead>
    <tbody>
		#foreach($app in $apps)
        <tr>
            <td>$app.id</td>
            <td>$app.name</td>
            <td>$app.creator</td>
            <td>$!datetool.format('yyyy-MM-dd HH:mm:ss',$!app.createTime)</td>
        </tr>
		#end
    </tbody>
</table>

#parse("/layout/alert.vm")

<script type="text/javascript" >

    function add() {
        window.location.href = "/app/add";
    }

</script>